            </ImageView>
            <Button fx:id="delb" layoutX="25.0" layoutY="494.0" mnemonicParsing="false" onAction="#delete" prefHeight="25.0" prefWidth="300.0" style="-fx-background-color: #ee586e; -fx-background-radius: 10;" stylesheets="@style.css" text="Supprimer" textFill="WHITE">
