                  <Label lineSpacing="20.0" text="Role :" textFill="#ee586e">
                     <font>
                        <Font name="Verdana Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <ImageView fx:id="pdp" fitHeight="98.0" fitWidth="104.0" layoutX="61.0" layoutY="85.0">
